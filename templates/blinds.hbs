<div class="blinds-inner component-inner">
	{{> component this}}
	<div class="blinds-widget component-widget">
		<div class="blinds-container">
			<div class="blinds-items clearfix">
				{{#each _items}}
					<div class="blinds-item" style="background-image:url('{{_graphic._src}}')" role="button" aria-label="{{_graphic.ariaLabel}}">
						<div class="blinds-text">
							{{a11y_text body}}
						</div>
					</div>
				{{/each}}
			</div>
		</div>
	</div>
</div>
